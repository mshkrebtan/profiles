alias ll='ls -alhF'
alias la='ls -1Ap'
alias l='ls -1Fp'
alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'

alias theme-light="export THEME=light"
alias theme-dark="export THEME=dark"

alias escape="sed 's/\ /\\\ /g; s/[(]/\\\(/g; s/[)]/\\\)/g; s/\[/\\\[/g; s/\]/\\\]/g'"
